package model;

public class Vaccum extends Appliance{
	private final String grade;
	public String grade()
	{
		return this.grade;
	}
	
	private final int batteryVotage;
	public int batteryVotage()
	{
		return this.batteryVotage;
	}
	
	
	private Vaccum(Builder builder)
	{
		super(builder);
		this.grade = builder.grade;
		this.batteryVotage = builder.batteryVotage;
	}
	
	public static class Builder extends Appliance.Builder<Builder> {
		private String grade;
		private int batteryVotage;
		
		public Vaccum build()
		{
			return new Vaccum(this);
		}
		
		public Builder grade(String grade)
		{
			return self();
		}
		
		public Builder batteryVotage(int batteryVotage)
		{
			return self();
		}

	}
}
